{
  "check-coverage": true,
  "per-file": true,
  "lines": 90,
  "statements": 90,
  "functions": 90,
  "branches": 90,
  "reporter": [
    "lcov",
    "text",
    "html"
  ],
  "report-dir": "./coverage",
  "cache": true,
  "all": true,
  "include": [
    "src/lib/*.js"
  ],
  "exclude": [
    "src/lib/process-banners.js"
  ]
}
